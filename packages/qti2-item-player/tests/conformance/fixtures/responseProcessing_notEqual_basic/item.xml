<?xml version="1.0" encoding="UTF-8"?>
<assessmentItem
  xmlns="http://www.imsglobal.org/xsd/imsqti_v2p2"
  identifier="responseProcessing_notEqual_basic"
  title="notEqual basic"
  adaptive="false"
  timeDependent="false">
  <responseDeclaration identifier="R" cardinality="single" baseType="integer"/>

  <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="integer">
    <defaultValue><value>0</value></defaultValue>
  </outcomeDeclaration>

  <itemBody>
    <p>Enter an integer.</p>
    <textEntryInteraction responseIdentifier="R" expectedLength="5"/>
  </itemBody>

  <responseProcessing>
    <responseCondition>
      <responseIf>
        <notEqual>
          <variable identifier="R"/>
          <baseValue baseType="integer">0</baseValue>
        </notEqual>
        <setOutcomeValue identifier="SCORE">
          <baseValue baseType="integer">1</baseValue>
        </setOutcomeValue>
      </responseIf>
      <responseElse>
        <setOutcomeValue identifier="SCORE">
          <baseValue baseType="integer">0</baseValue>
        </setOutcomeValue>
      </responseElse>
    </responseCondition>
  </responseProcessing>
</assessmentItem>


